    "watch": "gulp watch",
    "test": "gulp test",
    "migration:create": "node scripts/create-migration.js",
    "setup-hooks": "git config core.hooksPath .githooks",
    "check-schema": "node scripts/check-schema-changes.js",
    "deploy:digitalocean": "./deploy-digitalocean.sh",
    "deploy:cloudrun": "./deploy.sh",
