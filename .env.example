# Turnkey Configuration (Required for Authentication)
# Get these from https://app.turnkey.com after creating your organization
TURNKEY_ORGANIZATION_ID=your_organization_id_here
TURNKEY_API_PUBLIC_KEY=your_api_public_key_here
TURNKEY_API_PRIVATE_KEY=your_api_private_key_here
TURNKEY_AUTH_PROXY_PUBLIC_KEY=your_auth_proxy_public_key_here

# Optional: Custom Turnkey API URL (defaults to https://api.turnkey.com)
# TURNKEY_API_URL=https://api.turnkey.com

# Optional OAuth Client IDs (add when available)
# Get Google Client ID from https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=

# Future OAuth providers
DISCORD_CLIENT_ID=
APPLE_CLIENT_ID=